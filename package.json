{
  "name": "@mux/ai",
  "version": "0.1.0",
  "description": "AI library for Mux",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "example:chapters": "npx ts-node examples/chapters/basic-example.ts",
    "example:chapters:compare": "npx ts-node examples/chapters/provider-comparison.ts",
    "example:burned-in": "npx ts-node examples/burned-in-captions/basic-example.ts",
    "example:burned-in:compare": "npx ts-node examples/burned-in-captions/provider-comparison.ts",
    "example:summarization": "npx ts-node examples/summarization/basic-example.ts",
    "example:summarization:compare": "npx ts-node examples/summarization/provider-comparison.ts",
    "example:moderation": "npx ts-node examples/moderation/basic-example.ts",
    "example:moderation:compare": "npx ts-node examples/moderation/provider-comparison.ts",
    "example:translation": "npx ts-node examples/translation/basic-example.ts",
    "example:audio-translation": "npx ts-node examples/audio-translation/basic-example.ts"
  },
  "keywords": [
    "mux",
    "ai",
    "typescript"
  ],
  "author": "Mux",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muxinc/mux-ai.git"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@anthropic-ai/sdk": "^0.28.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/lib-storage": "^3.0.0",
    "@aws-sdk/s3-request-presigner": "^3.0.0",
    "@mux/mux-node": "^12.5.0",
    "dotenv": "^17.2.2",
    "openai": "^5.13.1",
    "p-retry": "^7.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "typescript": "^5.0.0"
  }
}
